<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!-- account_payment inherit form view -->
<record id="account_payment_view_form_inhe" model="ir.ui.view">
    <field name="name">account_payment.inherit.view.form</field>
    <field name="model">account.payment</field>
    <field name="inherit_id" ref="account.view_account_payment_invoice_form"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='payment_method_code']" position="after">
            <field name="tds" string="TDS"/>
        </xpath>
        
    </field>
</record>
</odoo>